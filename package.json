{
  "name": "accredify",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "build": "vite build && tsc --project ./tsconfig.server.json",
    "preview": "vite preview",
    "serve": "concurrently -k --raw \"yarn run dev:server\" \"yarn run dev:client\"",
    "start": "NODE_ENV=production ts-node server/app.ts",
    "dev:client": "vite",
    "build:client": "vite build",
    "dev:server": "ts-node server/app.ts",
    "build:server": "tsc --project ./tsconfig.server.json",
    "test:unit": "vitest --dom"
  },
  "dependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.17",
    "@types/mocha": "^10.0.1",
    "@vue/test-utils": "^2.3.2",
    "concurrently": "^8.0.1",
    "date-fns": "^2.30.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "happy-dom": "6.0.4",
    "nodemon": "^2.0.22",
    "pinia": "^2.0.35",
    "svg-sprite-loader": "^6.0.11",
    "ts-node": "^10.9.1",
    "vite-svg-loader": "^4.0.0",
    "vue": "^3.2.47",
    "vue-router": "4",
    "vue-svg-loader": "^0.16.0"
  },
  "devDependencies": {
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^5.59.2",
    "@typescript-eslint/parser": "^5.59.2",
    "@vitejs/plugin-vue": "^4.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/vue3-jest": "^29.2.4",
    "eslint": "^8.39.0",
    "eslint-plugin-vue": "^9.11.0",
    "sass": "^1.62.1",
    "typescript": "^5.0.2",
    "vite": "^4.3.2",
    "vitest": "^0.31.0",
    "vue-tsc": "^1.6.4"
  }
}
